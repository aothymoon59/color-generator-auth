{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react-recommended"],
  "rules": {
    "react-refresh/only-export-components": "warn",
    "no-unused-vars": 0,
    "no-undef": 0
  }
}
